return function(Window)
    local FarmTab = Window:CreateTab("Farm", "sprout")
    FarmTab:CreateSection("Coins")

    local actionEnabled = false

    local function sendCoinEvent()
        while actionEnabled do
            game:GetService("ReplicatedStorage").Events.CoinEvent:FireServer()
            wait(0.1)
        end
    end

    local Toggle = FarmTab:CreateToggle({
       Name = "Auto Coin Collect",
       CurrentValue = false,
       Flag = "Toggle1",
       Callback = function(Value)
           actionEnabled = Value
           if Value then
               coroutine.wrap(sendCoinEvent)()
           end
       end,
    })

local Toggle = FarmTab:CreateToggle({
   Name = "Dummy Damage",
   CurrentValue = false,
   Flag = "DummyDamageToggle",
   Callback = function(Value)
       if Value then
           _G.DummyDamageInterval = game:GetService("RunService").Heartbeat:Connect(function()
               if workspace.MAP.dummies:FindFirstChild("Dummy") then
                   game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(
                       workspace.MAP.dummies.Dummy.Humanoid,
                       14
                   )
               end
           end)
       else
           if _G.DummyDamageInterval then
               _G.DummyDamageInterval:Disconnect()
               _G.DummyDamageInterval = nil
           end
       end
   end,
})

end
