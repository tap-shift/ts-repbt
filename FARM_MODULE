return function(Window)
    local FarmTab = Window:CreateTab("Farm", "sprout")
    FarmTab:CreateSection("Coins")

    local actionEnabled = false

    local function sendCoinEvent()
        while actionEnabled do
            game:GetService("ReplicatedStorage").Events.CoinEvent:FireServer()
            wait(0.1)
        end
    end

    local Toggle = FarmTab:CreateToggle({
       Name = "Auto Coin Collect",
       CurrentValue = false,
       Flag = "Toggle1",
       Callback = function(Value)
           actionEnabled = Value
           if Value then
               coroutine.wrap(sendCoinEvent)()
           end
       end,
    })

    FarmTab:CreateSection("Bosses")

local Toggle = FarmTab:CreateToggle({
    Name = "LavaGorilla Killer",
    CurrentValue = false,
    Flag = "GorillaKillToggle",
    Callback = function(Value)
        if Value then
            _G.GorillaLoop = game:GetService("RunService").Heartbeat:Connect(function()
                game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(
                    workspace.NPC.LavaGorilla.Humanoid,
                    1
                )
            end)
        else
            if _G.GorillaLoop then
                _G.GorillaLoop:Disconnect()
                _G.GorillaLoop = nil
            end
        end
    end
})

end
