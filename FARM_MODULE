return function(Window)
    local FarmTab = Window:CreateTab("Farm", "sprout")
    FarmTab:CreateSection("Coins")

    local actionEnabled = false

    local function sendCoinEvent()
        while actionEnabled do
            game:GetService("ReplicatedStorage").Events.CoinEvent:FireServer()
            wait(0.1)
        end
    end

    local Toggle = FarmTab:CreateToggle({
       Name = "Auto Coin Collect",
       CurrentValue = false,
       Flag = "Toggle1",
       Callback = function(Value)
           actionEnabled = Value
           if Value then
               coroutine.wrap(sendCoinEvent)()
           end
       end,
    })

    FarmTab:CreateSection("Bosses")

local Toggle = FarmTab:CreateToggle({
    Name = "Auto Boss Kill",
    CurrentValue = false,
    Flag = "BossKillToggle",
    Callback = function(Value)
        if Value then
            _G.BossLoop = game:GetService("RunService").Heartbeat:Connect(function()
                local args = {
                    workspace.NPC.CENTAUR.Humanoid,
                    1
                }
                game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(unpack(args))
            end)
        else
            if _G.BossLoop then
                _G.BossLoop:Disconnect()
            end
        end
    end
})

end
